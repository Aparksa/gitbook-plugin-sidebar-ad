{
    "name": "gitbook-plugin-sidebar-pdf",
    "version": "0.0.1",
    "description": "GitBook plugin for adding link to pdf version in the sidebar of your book",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "license": "BSD",
    "repository": {
      "type": "git",
      "url": "https://github.com/aparksa/gitbook-plugin-sidebar-pdf.git"
    },
    "gitbook": {
        "properties": {
            "imageUrl": {
                "type": "string",
                "description": "pdf icon link",
                "required": false
            },
            "url": {
                "type": "string",
                "description": "URL of your published book"
            }
        }
    }
}
